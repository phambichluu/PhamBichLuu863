@model IEnumerable<PhamBichLuu863.Models.PBL863>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PBLID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PBLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PBLGender)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PBLID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PBLName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PBLGender)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PBLID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PBLID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PBLID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
